    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("coords finder v2", "DarkTheme")


    local Cords = Window:NewTab("coords finder")
    local Coords = Cords:NewSection("coords finder v2")
    local Positioning = tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.Position)
    
    Coords:NewButton("Copy Position", "", function()
        setclipboard(""..Positioning.."")
    end)
    
    Coords:NewButton("Generate Teleport Script", "", function()
        setclipboard("game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new("..Positioning..")")
    end)
